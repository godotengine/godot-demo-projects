[application]

config/name="Window Management"
config/description="A demo showing the various window management features available through DisplayServer."
config/tags=PackedStringArray("demo", "official", "porting")
run/main_scene="res://window_management.tscn"
config/features=PackedStringArray("4.2")
run/low_processor_mode=true
config/icon="res://icon.webp"

[debug]

